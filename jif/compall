wd=`dirname $0`
. ${wd}/../test/go
cd ${wd}
cd $BLD
gcc -O2 -Werror -Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -ansi zad.c field.c -o $BIN/zad
gcc -O2 -Werror -Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -ansi zcv.c field.c -o $BIN/zcv
gcc -O2 -Werror -Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -ansi zpr.c field.c -o $BIN/zpr
gcc -O2 -Werror -Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -ansi zte.c field.c -o $BIN/zte
gcc -O2 -Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -ansi -pthread zmu.c lay3.c lay2do.c pgcva.c pgcvb.c pgmul.c pgcvd.c field.c hpmi.c bkmad.c -o $BIN/zmu
