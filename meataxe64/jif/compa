mkdir -p $BIN
cd $SRC
gcc $1 -O3 -Wall -pthread -include malloc.h fieldreg.c -L$BIN -lmtx -o $BIN/fieldreg
gcc $1 -O3 -Wall -pthread -include malloc.h conreg.c -L$BIN -lmtx -o $BIN/conreg
