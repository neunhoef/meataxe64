gcc -O3 -Wall -pthread -include malloc.h zid.c mfuns.c -L$BIN -lmtx -o $BIN/zid
gcc_comp_options="-Wall -Wpointer-arith -Wwrite-strings -Winline -Waggregate-return -Wmissing-prototypes -Wnested-externs -W -Wunused -Wcast-align -Wstrict-prototypes -Wmissing-declarations -O3 -pthread -include malloc.h"
gcc_link_options="${gcc_comp_options} -L${BIN} -lmtx"
gcc -include mt_types.h zimport.c elements.c endian.c header.c primes.c utils.c read.c ${gcc_link_options} -lm -o $BIN/zimport
gcc -DEIGHT_BYTE_WORD -include mt_types.h zimport.c elements.c endian.c header.c primes.c utils.c read.c ${gcc_link_options} -lm -o $BIN/zimport64
gcc -include mt_types.h zexport.c elements.c endian.c header.c primes.c utils.c write.c ${gcc_link_options} -lm -o $BIN/zexport
gcc -DEIGHT_BYTE_WORD -include mt_types.h zexport.c elements.c endian.c header.c primes.c utils.c write.c ${gcc_link_options} -lm -o $BIN/zexport64
gcc -DEIGHT_BYTE_WORD -include mt_types.h zscript.c utils.c mfuns.c sums_utils.c ${gcc_link_options} -lm -o $BIN/zscript
gcc -DEIGHT_BYTE_WORD -include mt_types.h zsh.c utils.c mfuns.c sums_utils.c ${gcc_link_options} -lm -o $BIN/zsh
gcc -DEIGHT_BYTE_WORD -include mt_types.h zsums.c utils.c mfuns.c sums_utils.c primes.c ${gcc_link_options} -lm -o $BIN/zsums
gcc -DEIGHT_BYTE_WORD -include mt_types.h zdiff.c utils.c mfuns.c primes.c ${gcc_link_options} -lm -o $BIN/zdiff
