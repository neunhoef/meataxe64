#!/bin/sh
#
# $Id: bn,v 1.4 2006/03/14 19:34:38 jon Exp $
#
# Compute bn(x)
#
# Inputs
# $1: input
# $2: power
# $3: output
#
# Return codes
# 1: error in parameters
# 0: success
#
#set -x
#set -e
usage="$0: usage: $0 <mat> <n> <out>"
if [ 3 -ne $# ]; then
  echo $usage
  exit 1;
fi

a=$1
n=$2
o=$3
repeat=2
. yn
